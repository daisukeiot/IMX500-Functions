{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "console": {
      "isEnabled": false
    },
    "logLevel": {
      //"default": "Information"
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 1,
      "maxPollingInterval": "00:00:30"
    },
    "blobs": {
      "maxDegreeOfParallelism": 1
    }
  },
  "bindings": [
		{
			"type": "blobTrigger",
			"direction": "in",
			"connection": "BlobConnectionString",
			"path": "iothub-link/~{name}.jpg",
			"source": "logsAndContainerScan",
			"name": "myBlob"
		}
	]
}